# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'Recrutamento JYA iOS' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Recrutamento JYA iOS
	pod 'ObjectMapper'
	pod 'Alamofire'
	pod 'RxAlamofire'
        pod 'RxSwift', '~> 4.0'
        pod 'RxCocoa', '~> 4.0'
	pod 'IGListKit'
	pod 'Swinject'
	pod 'Kingfisher'

  target 'Recrutamento JYA iOSTests' do
    inherit! :search_paths
    # Pods for testing
        pod 'RxSwift', '~> 4.0'
	pod 'RxTest'
  end
end
